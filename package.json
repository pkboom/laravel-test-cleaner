{
    "name": "laravel-test-cleaner",
    "displayName": "Laravel Test Cleaner",
    "description": "Remove example test",
    "version": "0.0.1",
    "publisher": "pkboom",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:clean.test"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "clean.test",
                "title": "Clean Test"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pkboom/laravel-test-cleaner.git"
    }
}
